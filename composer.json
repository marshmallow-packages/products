{
    "name": "marshmallow/products",
    "description": "Package that will contain basic methods for handeling products and preparing them toe be used in ecommerce.",
    "keywords": [
        "marshmallow"
    ],
    "homepage": "https://github.com/Marshmallow-Development/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stef@marshmallow.dev"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "marshmallow/commands": "^1.0",
        "marshmallow/seoable": "^2.0",
        "marshmallow/sluggable": "^1.0",
        "marshmallow/helpers": "^2.0",
        "marshmallow/priceable": "^2.0",
        "marshmallow/nova-flexible": "^1.0",
        "marshmallow/feeds-google-shopping": "^1.0",
        "marshmallow/channels-channable": "^1.0",
        "eminiarts/nova-tabs": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Product\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\Product\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Product\\ProductServiceProvider"
            ],
            "aliases": {
                "Product": "Marshmallow\\Product\\Facades\\Product"
            }
        }
    }
}
